nents/Generation.js","original":{"line":86,"column":54}},{"generated":{"line":139,"column":101},"source":"components/Generation.js","original":{"line":86,"column":52}},{"generated":{"line":139,"column":103},"source":"components/Generation.js","original":{"line":86,"column":27}},{"generated":{"line":139,"column":104},"source":"components/Generation.js","original":{"line":86,"column":0}},{"name":"componentConnector","generated":{"line":141,"column":18},"source":"components/Generation.js","original":{"line":88,"column":15}},{"name":"Generation","generated":{"line":141,"column":37},"source":"components/Generation.js","original":{"line":88,"column":34}},{"generated":{"line":141,"column":47},"source":"components/Generation.js","original":{"line":88,"column":15}}],"sources":{"components/Generation.js":"import React, { Component } from \"react\";\nimport { connect } from \"react-redux\";\nimport { generationActionCreator } from \"../actions/generation\";\n\nconst DEFAULT_GENERATION = { generationId: \"\", expiration: \"\" };\nconst MINIMUN_DELAY = 3000;\n\nclass Generation extends Component {\n  state = { generation: DEFAULT_GENERATION };\n\n  timer = null;\n\n  componentDidMount() {\n    this.fetchNextGeneration();\n  }\n\n  componentWillUnmount() {\n    cleanTimeout(this.timer);\n  }\n\n  fetchGeneration = () => {\n    fetch(\"http://localhost:3000/generation\")\n      .then((response) => response.json())\n      .then((json) => {\n        //console.log(\"json\", json);\n        //this.setState({ generation: json.generation });\n\n        this.props.dispatchGeneration(json.generation);\n      })\n      .catch((error) => console.error(\"error\", error));\n  };\n\n  fetchNextGeneration = () => {\n    this.fetchGeneration();\n\n    let delay =\n      new Date(this.props.generation.expiration).getTime() - new Date().getTime;\n\n    if (delay < MINIMUN_DELAY) {\n      delay = MINIMUN_DELAY;\n    }\n\n    this.timer = setTimeout(() => this.fetchNextGeneration(), delay);\n  };\n\n  render() {\n    console.log(this.props);\n    //console.log(\"我先的\");\n    const { generation } = this.props;\n    return (\n      <div>\n        <h3>Generation {generation.generationId}. Expires on:</h3>\n        {/* <!-- 使用.toString()是因為new Date(generation.expiration)是一個物件沒辦法被render --> */}\n        <h4>{new Date(generation.expiration).toString()}</h4>\n      </div>\n    );\n  }\n}\n\n// 讓store裡的state可以傳進來\nconst mapStateToProps = (state) => {\n  const generation = state.generation;\n\n  return { generation };\n};\n\n// 使Generation有dispatchGeneration為props，可以用來dispatch generaiton\n// const mapDispatchToProps = (dispatch) => {\n//   return {\n//     fetchGeneration: () => fetchGeneration(dispatch)\n//   };\n// };\n\nconst fetchGeneration = () => (dispatch) => {\n  fetch(\"http://localhost:3000/generation\")\n    .then((response) => response.json())\n    .then((json) => {\n      //console.log(\"json\", json);\n      //this.setState({ generation: json.generation });\n\n      this.props.dispatchGeneration(json.generation);\n    })\n    .catch((error) => console.error(\"error\", error));\n};\n\nconst componentConnector = connect(mapStateToProps, { fetchGeneration });\n\nexport default componentConnector(Generation);\n"},"lineCount":null}},"hash":"4039b228ecf46b4ca0ad6ca4c41a5b25","cacheData":{"env":{}}}